{
  "name": "ts-mockito",
  "version": "1.2.0",
  "description": "Mocking library for TypeScript",
  "main": "lib/ts-mockito.js",
  "typings": "lib/ts-mockito",
  "scripts": {
    "compile": "rm -rf lib && ./node_modules/.bin/tsc -p ./src",
    "test": "npm run compile && ./node_modules/.bin/karma start karma.conf.js --single-run",
    "test:watch": "npm run compile && ./node_modules/.bin/karma start karma.conf.js"
  },
  "author": "kuster.maciej@gmail.com",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/NagRock/ts-mockito"
  },
  "keywords": [
    "mock",
    "typescript",
    "tests",
    "fake",
    "stub",
    "spy",
    "javascript"
  ],
  "devDependencies": {
    "@types/jasmine": "^2.2.33",
    "@types/lodash": "^4.14.34",
    "@types/node": "^6.0.38",
    "awesome-typescript-loader": "^3.1.2",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "sourcemap": "^0.1.0",
    "ts-helpers": "^1.1.1",
    "typescript": "^2.2.2",
    "webpack": "^2.3.2"
  },
  "dependencies": {
    "lodash": "^4.15.0"
  }
}
